<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="paintGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#059669;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#10b981;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="400" height="300" fill="url(#paintGrad)"/>
  
  <!-- Painting elements -->
  <g fill="#ffffff" opacity="0.4">
    <!-- Paint roller -->
    <rect x="150" y="100" width="100" height="15" rx="7"/>
    <rect x="145" y="95" width="10" height="25" rx="5"/>
    <line x1="155" y1="120" x2="155" y2="180" stroke="#ffffff" stroke-width="3"/>
    
    <!-- Paint brush -->
    <rect x="280" y="120" width="40" height="8" rx="4" transform="rotate(30 300 124)"/>
    <rect x="275" y="115" width="8" height="20" rx="4" transform="rotate(30 279 125)"/>
    
    <!-- Paint can -->
    <rect x="80" y="180" width="60" height="80" rx="5"/>
    <ellipse cx="110" cy="180" rx="30" ry="8" fill="#ffffff"/>
    
    <!-- Wall with paint -->
    <rect x="20" y="50" width="360" height="200" fill="none" stroke="#ffffff" stroke-width="2" opacity="0.6"/>
    <rect x="20" y="50" width="180" height="200" fill="#ffffff" opacity="0.1"/>
  </g>
  
  <!-- Icon -->
  <g fill="#ffffff" opacity="0.8">
    <rect x="180" y="40" width="40" height="8" rx="4"/>
    <rect x="175" y="35" width="8" height="18" rx="4"/>
    <line x1="183" y1="53" x2="183" y2="80" stroke="#ffffff" stroke-width="2"/>
  </g>
  
  <!-- Text -->
  <text x="200" y="25" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#ffffff">
    Professional Painting
  </text>
  <text x="200" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#ffffff" opacity="0.6">
    Service Placeholder - Replace with painting work image
  </text>
</svg>
